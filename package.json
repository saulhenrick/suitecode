{
    "name": "suitecode",
    "displayName": "suitecode",
    "description": "",
    "version": "0.0.1",
    "publisher": "saulhenrick",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.setupProject"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.setupProject",
            "title": "SuiteCode: Setup"
        }],
        "configuration": {
            "properties": {
                "suitecode.passport": {
                    "type": "object"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}